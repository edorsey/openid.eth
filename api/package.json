{
  "name": "api.openidconnect.eth",
  "private": true,
  "version": "0.0.0",
  "bin": {
    "hydra-login-consent-logout": "lib/app.js"
  },
  "prettier": "ory-prettier-styles",
  "config": {
    "prettierTarget": "{src,views}/{*,**/*}.{ts,js,json}"
  },
  "scripts": {
    "build": "tsc",
    "serve": "node lib/app.js",
    "start": "ts-node-dev --watch public,views --respawn src/app.ts",
    "test": "npm-run-all build",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}"
  },
  "dependencies": {
    "@oryd/hydra-client": "1.9.0-alpha.2",
    "@prettier/plugin-pug": "^1.16.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/url-join": "^4.0.0",
    "base64url": "^3.0.1",
    "body-parser": "^1.19.0",
    "bulma": "^0.9.3",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "debug": "^4.1.1",
    "ethers": "^5.4.7",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "pug": "^2.0.4",
    "querystring": "^0.2.0",
    "serve-favicon": "^2.5.0",
    "typescript": "^4.4.3",
    "url-join": "^4.0.1",
    "webauthn": "^0.2.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.4",
    "npm-run-all": "^4.1.5",
    "ory-prettier-styles": "1.1.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.8"
  }
}
